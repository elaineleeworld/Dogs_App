<%= form_for @dog do |f| %>
	Name: <%= f.text_field :name %>
	<br>
	Age: <%= f.text_field :age %>
	<br>
	Breed: <%= f.text_field :breed %>
	<br>
	City: <%= f.text_field :city %>
	<br>
	Weight: <%= f.text_field :weight %>
	<br>
	Aggressive? <%= f.text_field :aggressive %>
	<br>
	Good with kids? <%= f.text_field :kids %>
	<br>
	Updated with shots? <%= f.text_field :shots %>
	<br>
	Play Style: <%= f.text_field :playstyle %>
	<br>
	Funny Fact: <%= f.text_field :funnyfact %>
	<br>
	Additional Info: <%= f.text_field :info %>
	<br>
	<br>

	<%= f.submit "Submit!" %>
	<br>

<% end %>
